{
  "manifest_version": 2,
  "name": "tv-schedule-parser",
  "description": "tv-schedule-parser",
  "minimum_chrome_version": "54",
  "version": "1.0.0",
  "icons": {},
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": ["main.js"],
      "run_at": "document_end"
    }
  ]
}
